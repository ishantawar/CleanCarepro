{
  "name": "cleancare-pro",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:backend": "cd backend && node server-laundry.js",
    "dev:full": "concurrently \"npm run dev\" \"npm run dev:backend\"",
    "build": "vite build && node scripts/optimize-build.js",
    "build:check": "tsc -b && vite build && node scripts/optimize-build.js",
    "build:analyze": "vite build --mode production && node scripts/bundle-analyzer.js",
    "lint": "eslint .",
    "preview": "vite preview",
    "start": "npm run start:prod",
    "start:prod": "npm run build && cd backend && npm install --production && NODE_ENV=production node server-laundry.js",

    "start:backend": "NODE_ENV=production node backend/server-laundry.js",
    "start:frontend": "npx serve -s dist -p 8080",
    "deploy": "chmod +x deploy.sh && ./deploy.sh",
    "render:build": "npm install && npm run build && cd backend && npm install --production && echo 'Build completed successfully'",
    "render:start": "NODE_ENV=production node backend/server-laundry.js",
    "install:all": "npm install && cd backend && npm install",
    "build:production": "npm run install:all && npm run build"
  },
